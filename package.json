{
  "name": "io_messenger",
  "author": "Futurice",
  "description": "React Native App Starter Kit for Android and iOS",
  "license": "MIT",
  "homepage": "http://getpepperoni.com",
  "bugs": "https://github.com/futurice/pepperoni-app-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/futurice/pepperoni-app-kit"
  },
  "keywords": [
    "React Native",
    "Starter Kit",
    "Bitrise",
    "Redux",
    "Auth0"
  ],
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "bundle:ios": "node ./node_modules/react-native/local-cli/cli.js bundle --platform ios --entry-file index.ios.js --bundle-output ios/io_messenger/main.jsbundle --dev=false --verbose",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src test",
    "beautify": "eslint --fix src",
    "coverage": "rimraf coverage && jest --coverage",
    "version": "support/version-ios.sh"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ]
  },
  "dependencies": {
    "babel-preset-react-native": "^4.0.0",
    "bluebird": "^3.3.5",
    "event-emitter": "^0.3.4",
    "immutable": "^3.7.6",
    "iota.lib.js": "l3wi/iota.lib.js",
    "lodash": "^4.11.0",
    "moment": "^2.12.0",
    "native-base": "^2.3.3",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-native": "^0.49.3",
    "react-native-contacts": "^1.0.3",
    "react-native-crypto": "^2.0.2",
    "react-native-elements": "^0.17.0",
    "react-native-firebase": "^3.0.5",
    "react-native-gifted-chat": "^0.2.9",
    "react-native-maps": "^0.17.1",
    "react-native-nav": "^2.0.2",
    "react-native-phone-input": "^0.1.10",
    "react-native-vector-icons": "^4.0.0",
    "react-navigation": "^1.0.0-beta.9",
    "react-redux": "^5.0.6",
    "readable-stream": "2.3.3",
    "redux": "^3.4.0",
    "redux-logger": "^3.0.6",
    "redux-loop-symbol-ponyfill": "^2.2.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.1",
    "standard-http-error": "^2.0.0",
    "stream-browserify": "^2.0.1",
    "superagent": "^3.8.0",
    "vm-browserify": "0.0.4",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.4.0",
    "fetch-mock": "^5.5.0",
    "istanbul": "1.0.0-alpha.2",
    "jest": "^21.2.1",
    "plop": "^1.9.0",
    "react-addons-test-utils": "~15.6.2",
    "react-native-mock": "~0.3.1",
    "react-test-renderer": "^16.0.0",
    "remote-redux-devtools": "^0.5.7",
    "rimraf": "^2.5.2"
  },
  "react-native": {
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "vm": "vm-browserify"
  }
}
